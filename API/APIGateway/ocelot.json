{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/customers/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7166
        }
      ],
      "UpstreamPathTemplate": "/gateway/customers/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "customers"
    },
    {
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7101
        }
      ],
      "UpstreamPathTemplate": "/gateway/products/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "products"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "customers",
      "Config": [
        {
          "Name": "Customers API",
          "Version": "v1",
          "Url": "https://localhost:7166/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "products",
      "Config": [
        {
          "Name": "Products API",
          "Version": "v1",
          "Url": "https://localhost:7101/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7141"
  }
}